<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SeekBarView">
        <!--max progress -->
        <attr name="maxProgress" format="integer"/>
        <!-- min minProgress -->
        <attr name="minProgress" format="integer"/>
        <!-- seekbar width needs <= seekbar veiw layout_width -->
        <attr name="width" format="dimension"/>
        <!-- seekbar height needs <= seekbar veiw layout_height -->
        <attr name="height" format="dimension"/>
        <!-- 是否 0 在中心 -->
        <attr name="centerState" format="boolean"/>
        <!-- 水平/垂直 切换 默认垂直-->
        <attr name="VerticalState" format="boolean"/>
        <!-- 是否 自动归0 -->
        <attr name="AutoZero" format="boolean"/>
        <!-- 背景 拉条杆 底色 -->
        <attr name="backColor" format="color"/>
        <!-- 背景 拉条杆 Width -->
        <attr name="backWidth" format="dimension"/>
        <!-- 背景 拉条杆 height -->
        <attr name="backHeight" format="dimension"/>
        <!-- 背景 圆角边框 描边-->
        <attr name="backFrameColor" format="color"/>
        <!-- 背景 圆角边框 宽度 -->
        <attr name="backFrameSize" format="dimension"/>
        <!--progress 进度 颜色-->
        <attr name="progressColor" format="color"/>
        <!--progress Width -->
        <attr name="progressWidth" format="dimension"/>
        <!--progress height -->
        <attr name="progressHeight" format="dimension"/>
        <!-- 0在中心 时  负数 的颜色 -->
        <attr name="progressMinusColor" format="color"/>
        <!--当前 progress-->
        <attr name="progress" format="integer"/>
        <!-- thumb 圆 半径 -->
        <attr name="thumbNormalRadius" format="dimension"/>
        <!-- thumb 圆 触摸时 半径 -->
        <attr name="thumbPressRadius" format="dimension"/>
        <!-- thumb 圆 颜色 -->
        <attr name="thumbColor" format="color"/>
        <attr name="textLocation" format="dimension"/>
        <!--进度字体大小-->
        <attr name="textSize" format="dimension"/>
        <!--进度字体 颜色-->
        <attr name="textColor" format="color"/>
        <!--进度字体 背景 颜色-->
        <attr name="textBackColor" format="color"/>
        <!--进度字体背景 圆角矩形 弧度-->
        <attr name="textBackRadius" format="dimension"/>
    </declare-styleable>
</resources>